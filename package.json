{
  "name": "auth-boilerplate",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henri-hulski/auth-boilerplate.git"
  },
  "keywords": [
    "cerebral"
  ],
  "author": "Henri Hulski <henri.hulski@gazeta.pl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henri-hulski/auth-boilerplate/issues"
  },
  "homepage": "https://github.com/henri-hulski/auth-boilerplate#readme",
  "scripts": {
    "start": "npm run build&&env/bin/gunicorn server.run",
    "build": "webpack --config webpack.config.js --progress",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js --progress",
    "test": "ava -v",
    "coverage": "nyc ava",
    "coverage:report": "nyc report --reporter=html",
    "precommit": "lint-staged",
    "commitmsg": "cz-customizable-ghooks",
    "commit": "git-cz",
    "lint": "eslint \"client/**/*.js\"",
    "format": "npm run lint -- --fix"
  },
  "dependencies": {
    "@cerebral/forms": "^1.1.0",
    "@cerebral/http": "^2.0.0",
    "@cerebral/router": "^1.1.0",
    "@cerebral/storage": "^1.1.0",
    "cerebral": "^2.2.0",
    "es6-error": "^4.0.2",
    "jwt-decode": "^2.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.72.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.7",
    "cz-customizable": "^5.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.11.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "mock-local-storage": "1.0.2",
    "nyc": "^11.2.1",
    "prettier": "^1.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "xhr-mock": "^1.9.1"
  },
  "lint-staged": {
    "client/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "ava": {
    "files": [
      "client/**/*.test.js"
    ],
    "source": [
      "client/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register",
      "babel-polyfill",
      "mock-local-storage"
    ]
  }
}
