{
  "name": "auth-boilerplate",
  "version": "1.1.0-dev0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henri-hulski/auth-boilerplate.git"
  },
  "keywords": [
    "cerebral"
  ],
  "author": "Henri Hulski <henri.hulski@gazeta.pl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henri-hulski/auth-boilerplate/issues"
  },
  "homepage": "https://github.com/henri-hulski/auth-boilerplate#readme",
  "scripts": {
    "build": "parcel build -d build static/index.html --no-cache",
    "commit": "git-cz",
    "commitmsg": "cz-customizable-ghooks",
    "coverage": "jest --coverage",
    "format": "npm run lint -- --fix && prettier --write \"*.md\" \"docs/**/*.md\" \"*.json\"",
    "lint": "eslint \"client/**/*.js\"",
    "precommit": "lint-staged",
    "start": "env/bin/gunicorn server.run&&parcel serve static/index.html --open",
    "test": "jest"
  },
  "dependencies": {
    "@cerebral/forms": "^3.2.0",
    "@cerebral/http": "^4.2.0",
    "@cerebral/react": "^3.2.0",
    "@cerebral/router": "^3.2.0",
    "@cerebral/storage": "^4.2.0",
    "cerebral": "^4.2.0",
    "jwt-decode": "^2.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-tap-event-plugin": "^3.0.2",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.11",
    "cz-customizable": "^5.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.13",
    "xhr-mock": "^2.3.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "client/app/**/*.js"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/client/setupTests.js",
    "testMatch": [
      "<rootDir>/client/**/*.test.js"
    ],
    "testURL": "http://localhost"
  }
}
