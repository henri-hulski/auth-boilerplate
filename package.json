{
  "name": "auth-boilerplate",
  "version": "1.1.0-dev0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henri-hulski/auth-boilerplate.git"
  },
  "keywords": [
    "cerebral"
  ],
  "author": "Henri Hulski <henri.hulski@gazeta.pl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henri-hulski/auth-boilerplate/issues"
  },
  "homepage": "https://github.com/henri-hulski/auth-boilerplate#readme",
  "scripts": {
    "build": "parcel build -d build static/index.html --no-cache",
    "commit": "git-cz",
    "commitmsg": "cz-customizable-ghooks",
    "coverage": "jest --coverage",
    "format": "npm run lint -- --fix && prettier --write \"*.md\" \"docs/**/*.md\" \"*.json\"",
    "lint": "eslint \"client/**/*.js\"",
    "precommit": "lint-staged",
    "start": "env/bin/gunicorn server.run&&parcel serve static/index.html --open",
    "test": "jest"
  },
  "dependencies": {
    "@cerebral/forms": "^3.2.2",
    "@cerebral/http": "^4.2.3",
    "@cerebral/react": "^3.2.2",
    "@cerebral/router": "^3.2.2",
    "@cerebral/storage": "^4.2.2",
    "cerebral": "^4.2.2",
    "jwt-decode": "^2.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-tap-event-plugin": "^3.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "commitizen": "^4.0.5",
    "cz-customizable": "^6.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-localstorage-mock": "^2.4.2",
    "lint-staged": "^10.2.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "xhr-mock": "^2.5.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "client/app/**/*.js"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/client/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/client/**/*.test.js"
    ],
    "testURL": "http://localhost"
  }
}
