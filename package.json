{
  "name": "auth-boilerplate",
  "version": "1.1.0-dev0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henri-hulski/auth-boilerplate.git"
  },
  "keywords": [
    "cerebral"
  ],
  "author": "Henri Hulski <henri.hulski@gazeta.pl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henri-hulski/auth-boilerplate/issues"
  },
  "homepage": "https://github.com/henri-hulski/auth-boilerplate#readme",
  "scripts": {
    "start": "npm run build&&env/bin/gunicorn server.run",
    "build": "webpack --config webpack.config.js --progress",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js --progress",
    "test": "jest",
    "coverage": "jest --coverage",
    "precommit": "lint-staged",
    "commitmsg": "cz-customizable-ghooks",
    "commit": "git-cz",
    "lint": "eslint \"client/**/*.js\"",
    "format": "npm run lint -- --fix"
  },
  "dependencies": {
    "@cerebral/forms": "^3.1.1",
    "@cerebral/http": "^4.1.1",
    "@cerebral/react": "^3.1.1",
    "@cerebral/router": "^3.1.1",
    "@cerebral/storage": "^4.1.1",
    "cerebral": "^4.1.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-tap-event-plugin": "^3.0.2",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-react": "^0.78.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.9",
    "cz-customizable": "^5.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "jest": "^22.2.1",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "xhr-mock": "^2.0.3"
  },
  "lint-staged": {
    "client/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "client/app/**/*.js"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/client/setupTests.js",
    "testMatch": [
      "<rootDir>/client/**/*.test.js"
    ],
    "testURL": "http://localhost"
  }
}
