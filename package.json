{
  "name": "auth-boilerplate",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henri-hulski/auth-boilerplate.git"
  },
  "keywords": [
    "cerebral"
  ],
  "author": "Henri Hulski <henri.hulski@gazeta.pl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henri-hulski/auth-boilerplate/issues"
  },
  "homepage": "https://github.com/henri-hulski/auth-boilerplate#readme",
  "scripts": {
    "start": "npm run build&&env/bin/gunicorn server.run",
    "build": "webpack --config webpack.config.js --progress",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js --progress",
    "test": "tap client/**/*.test.js --node-arg=-r --node-arg=babel-core/register --node-arg=-r --node-arg=mock-local-storage",
    "coverage": "nyc --require=babel-core/register --require=mock-local-storage tap client/**/*.test.js",
    "coverage:report": "nyc report --reporter=html"

  },
  "dependencies": {
    "cerebral": "2.0.0-b-alpha.d6efd5f3",
    "cerebral-forms": "1.0.0-b-alpha.d6efd5f3",
    "cerebral-provider-http": "1.0.0-b-alpha.d6efd5f3",
    "cerebral-router": "1.0.0-4-router-great-again",
    "jwt-decode": "^2.1.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "semantic-ui-react": "^0.67.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.26.2",
    "file-loader": "^0.10.1",
    "js-mock": "^1.0.0",
    "mock-local-storage": "^1.0.2",
    "sinon": "^2.0.0-pre.6",
    "standard": "^9.0.0",
    "style-loader": "^0.13.2",
    "tap": "^10.3.0",
    "webpack": "^2.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/webpack.*",
      "*.spec.js"
    ],
    "globals": [
      "localStorage",
      "URLSearchParams",
      "location"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    }
  }
}
